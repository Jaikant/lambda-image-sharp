{
  "name": "lambda-image-sharp",
  "version": "0.0.1",
  "description": "Lambda Image Resize With Sharp",
  "main": "handler.js",
  "engines": {
    "node": ">= 6.10.0"
  },
  "config": {
    "jsSrc": "src/"
  },
  "scripts": {
    "deploy": "serverless deploy -v"
  },
  "keywords": [
    "serverless",
    "sharp",
    "libvips",
    "image",
    "resize",
    "optimize"
  ],
  "author": "Jaikant Kumaran",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.80.0",
    "babel-runtime": "^6.23.0",
    "image-size": "0.6.2",
    "mime-types": "^2.1.15",
    "prettier": "^1.5.2",
    "sharp": "^0.18.2",
    "sprintf-js": "^1.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.24.1",
    "decompress": "^4.2.0",
    "eslint": "^4.1.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-ava": "^4.2.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-tap": "^2.0.1",
    "fs-extra": "^3.0.1",
    "json-loader": "0.5.4",
    "serverless": "^1.16.1",
    "serverless-webpack": "^1.0.0-rc.4",
    "webpack": "3.10.0"
  },
  "babel": {
    "sourceMaps": "inline",
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "6.10"
          }
        }
      ],
      "stage-3"
    ]
  }
}
