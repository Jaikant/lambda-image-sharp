{
  "all": [
    ["rotate"]
  ],
  "key": "%(filename)s-%(sz)s.jpg",
  "params": {
    "ACL": "public-read"
  },
  "output" : {
    "default" : {
       "outputdir": "defaulttest/",
       "maxWidth": "800"
     },
    "cover" : {
       "outputdir": "cover/",
       "maxWidth": "800",
       "maxHeight": "240"
     },
    "trekgram-incoming" : {
       "outputdir": "trekgram/",
       "transformtype" : "responsive",
       "maxWidth": "292",
       "maxHeight": "292"
     }
  },
  "operations": [
    ["resize"],
    ["max"],
    ["withoutEnlargement"]
  ]
}
